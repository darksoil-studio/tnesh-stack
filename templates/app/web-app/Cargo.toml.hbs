[profile.dev]
opt-level = "z"

[profile.release]
opt-level = "z"

[workspace]
members = ["dnas/*/zomes/coordinator/*", "dnas/*/zomes/integrity/*"]
resolver = "2"

[workspace.dependencies]
holochain = { version = "{{holochain_version}}", default-features = false, features = [
  "test_utils",
] }
hdi = "{{hdi_version}}"
hdk = "{{hdk_version}}"
