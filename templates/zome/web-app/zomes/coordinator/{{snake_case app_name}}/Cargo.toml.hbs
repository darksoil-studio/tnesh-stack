[package]
name = "{{snake_case app_name}}"
version = "0.0.1"
edition = "2021"

[lib]
crate-type = ["cdylib", "rlib"]
name = "{{snake_case app_name}}"

[dependencies]
hdk = { workspace = true }

serde = { workspace = true }

{{snake_case app_name}}_integrity = { path = "../../integrity/{{snake_case app_name}}", package = "{{snake_case app_name}}_integrity" }
